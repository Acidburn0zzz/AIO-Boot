color black/cyan yellow/cyan
write 0x8274 0x2001
timeout 10
ls /WINSETUP/grub4dos/windefault > nul && default /WINSETUP/grub4dos/windefault

title ...Back to Grub2
ls /WINSETUP/grub4dos/usbdrive.tag > nul || find --set-root --devices=hf /WINSETUP/grub4dos/usbdrive.tag > nul
chainloader /Boot/grub/g2ldr

title \n
root

title Auto-detect and use F6 SATA/RAID/SCSI Driver\nUse if your internal disk is not detected or getting BSOD 0x0000007B\nSelect once before Text mode of Setup or starting PE1\nCurrently 32 bit versions are supported.\nCreated by Chenall (www.chenall.net/) & Modified by Steve Si (www.rmprepusb.com)
#http://bbs.wuyou.com/viewthread.php?tid=197550
#http://chenall.net/
#http://www.rmprepusb.com
map --unmap=0:0xFF
map --unhook && map --rehook
ls /WINSETUP/grub4dos/DPMS/DPMS.ISO > nul || find --set-root --devices=hf /WINSETUP/grub4dos/DPMS/DPMS.ISO
map /WINSETUP/grub4dos/DPMS/DPMS.ISO (0xfa)|| map --mem /WINSETUP/grub4dos/DPMS/DPMS.ISO (0xfa)
map --hook
root (0xfa)
set FIRA=0
/dpms2.g4b 0
ls (0)/txtsetup.oem && map (0) (1) && set DPMS=1
map --unmap=0xfa
map --rehook
ls /WINSETUP/grub4dos/usbdrive.tag > nul || find --set-root --devices=hf /WINSETUP/grub4dos/usbdrive.tag
configfile /WINSETUP/grub4dos/menu.lst

title Auto-detect and use F6 SATA/RAID/SCSI Driver + Firadisk\nTry this entry if the one above does not work on particular machine\nUse once, before Text mode of Setup
#http://bbs.wuyou.com/viewthread.php?tid=197550
#http://chenall.net/
#http://www.rmprepusb.com
map --unmap=0:0xFF
map --unhook && map --rehook
ls /WINSETUP/grub4dos/DPMS/DPMS.ISO > nul || find --set-root --devices=hf /WINSETUP/grub4dos/DPMS/DPMS.ISO
map /WINSETUP/grub4dos/DPMS/DPMS.ISO (0xfa)|| map --mem /WINSETUP/grub4dos/DPMS/DPMS.ISO (0xfa)
map --hook
root (0xfa)
set FIRA=0
/dpms2.g4b 0
ls (0)/txtsetup.oem && map (0) (1)
map --unmap=0xfa
map --rehook
ls (0)/txtsetup.oem && set LOADFIRA=1 && set DPMS=1
ls /WINSETUP/grub4dos/usbdrive.tag > nul || find --set-root --devices=hf /WINSETUP/grub4dos/usbdrive.tag
configfile /WINSETUP/grub4dos/menu.lst

