title %backlst%
	fallback +1
	find --set-root --ignore-floppies --ignore-cd /Boot/grub4dos/menu.lst
	configfile /Boot/grub4dos/menu.lst

title Load Grub2
	chainloader /Boot/grub/g2ldr

title Load Syslinux
	chainloader +1

title %reboot%
	reboot

title %halt%
	halt

title %mcmd%
	commandline

