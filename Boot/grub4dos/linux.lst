set UUID=%?%
kernel	(0xff)/images/pxeboot/vmlinuz inst.stage2=hd:UUID=%UUID%:/Files/Linux/CentOS-7-x86_64-Minimal.iso quiet
initrd	(0xff)/images/pxeboot/initrd.img

